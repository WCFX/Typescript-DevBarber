{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": "src",
    "paths": {
      "~/*": ["*"]
    }
  }
}